<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">



<generatorConfiguration> 
   
  <context id="mysqlTables" targetRuntime="MyBatis3">
    <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://10.100.142.11:3306/godzilla_db"
     userId="root" password="root"/>
    
    <!-- 指定生成的类型为java类型，避免数据库中number等类型字段 -->
    <javaTypeResolver>
      <property name="forceBigDecimals" value="false"/>
    </javaTypeResolver>
    
    <!-- 生成model模型，对应的包，存放位置可以指定具体的路径,如/ProjectName/src，也可以使用MAVEN来自动生成 -->
    <javaModelGenerator targetPackage="cn.creditease.godzilla.model" targetProject="MAVEN">
      <property name="enableSubPackages" value="true"/>
    </javaModelGenerator>
    
    <!--对应的xml mapper文件  -->
    <sqlMapGenerator targetPackage="cn.creditease.godzilla.dao" targetProject="MAVEN">
      <property name="enableSubPackages" value="true"/>
    </sqlMapGenerator>
    
    <!-- 对应的dao接口 -->
    <javaClientGenerator type="XMLMAPPER" targetPackage="cn.creditease.godzilla.dao" targetProject="MAVEN">
      <property name="enableSubPackages" value="true"/>
    </javaClientGenerator>
    
    
    <table tableName="t_g_user" domainObjectName="User" enableCountByExample="false" enableSelectByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"/>
  	<table tableName="project_status" domainObjectName="ProjStatus" enableCountByExample="false" enableSelectByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"/>
  	<table tableName="t_g_project" domainObjectName="Project" enableCountByExample="false" enableSelectByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"/>
  	<table tableName="t_g_function_right" domainObjectName="FunRight" enableCountByExample="false" enableSelectByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"/>
  	<table tableName="t_g_properties_config" domainObjectName="PropConfig" enableCountByExample="false" enableSelectByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"/>
  	<table tableName="t_g_project_status_history" domainObjectName="ProjStatus" enableCountByExample="false" enableSelectByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"/>
  	<table tableName="t_g_project_private" domainObjectName="ProjPrivate" enableCountByExample="false" enableSelectByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"/>
  	<table tableName="t_g_operate_logs" domainObjectName="OperateLog" enableCountByExample="false" enableSelectByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"/>
  	<table tableName="t_g_svn_conflict" domainObjectName="SvnConflict" enableCountByExample="false" enableSelectByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"/>
  	<table tableName="t_g_maven_command_logs" domainObjectName="MvnCmdLog" enableCountByExample="false" enableSelectByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"/>
  	<table tableName="t_g_svn_change_logs" domainObjectName="SvnChangeLog" enableCountByExample="false" enableSelectByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"/>
  	<table tableName="t_g_svn_command_logs" domainObjectName="SvnCmdLog" enableCountByExample="false" enableSelectByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"/>
  	
  </context>
</generatorConfiguration>